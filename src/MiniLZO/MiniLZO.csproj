<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <Nullable>enable</Nullable>
    <Title>MiniLZO for .NET</Title>
    <Version>1.0.1.0</Version>
    <Authors>Henk van Grootheest</Authors>
    <Company />
    <Description>MiniLZO for .NET is a robust and cross-platform C# reimplementation of the C miniLZO library</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/Ahrkylien/MiniLZO</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>lzo;minilzo;lzomini;</PackageTags>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
